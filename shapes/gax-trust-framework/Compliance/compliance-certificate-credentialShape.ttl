@prefix gax-trust-framework: <http://w3id.org/gaia-x/gax-trust-framework#> .
@prefix gax-validation: <http://w3id.org/gaia-x/validation#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

gax-validation:ComplianceCertificateCredentialShape a sh:NodeShape ;
    sh:property [ skos:example "'https://company-a.com/self-descriptions/cab.jsonld'" ;
            sh:description "ID des Anspruchs, in einem überprüfbaren Anmeldeinformationsbau mit allen Informationen unterzeichnet zu werden, die in der Forderung verbunden sind.Im Falle einer Anmeldeinformation von Drittanbietern ist die ID die ID des Dritten"@de,
                "ID of the claim to be signed in a verifiable credential build with all the information that are bound in the claim. In case of third party credential the ID is the ID of the third party"@en ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "credential subject" ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path gax-trust-framework:credentialSubject ] ;
    sh:targetClass gax-trust-framework:ComplianceCertificateCredential .

