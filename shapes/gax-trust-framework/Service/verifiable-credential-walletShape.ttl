@prefix gax-trust-framework: <http://w3id.org/gaia-x/gax-trust-framework#> .
@prefix gax-validation: <http://w3id.org/gaia-x/validation#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

gax-validation:VerifiableCredentialWalletShape a sh:NodeShape ;
    sh:property [ skos:example "'https://gaia-x.eu'" ;
            sh:datatype xsd:string ;
            sh:description "Eine Liste des maschinenlesbaren Formats, der zum Exportüberprüfungsnachweis verwendet wird."@de,
                "a list of machine readable format used to export verifiable credentials."@en ;
            sh:minCount 1 ;
            sh:name "verifiable credential export format" ;
            sh:order 1 ;
            sh:path gax-trust-framework:verifiableCredentialExportFormat ],
        [ skos:example "'https://gaia-x.eu'" ;
            sh:datatype xsd:string ;
            sh:description "Eine Liste der maschinenlesbaren Format, mit der private Schlüssel exportiert werden."@de,
                "a list of machine readable format used to export private keys."@en ;
            sh:minCount 1 ;
            sh:name "private key export format" ;
            sh:order 2 ;
            sh:path gax-trust-framework:privateKeyExportFormat ] ;
    sh:targetClass gax-trust-framework:VerifiableCredentialWallet .

